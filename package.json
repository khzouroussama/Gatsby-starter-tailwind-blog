{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "4.0.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "framer-motion": "^1.11.0",
    "gatsby": "2.23.1",
    "gatsby-image": "^2.4.6",
    "gatsby-plugin-emotion": "^4.3.3",
    "gatsby-plugin-manifest": "2.4.10",
    "gatsby-plugin-netlify-cms": "^4.3.5",
    "gatsby-plugin-offline": "3.2.8",
    "gatsby-plugin-postcss": "2.3.2",
    "gatsby-plugin-react-helmet": "3.3.3",
    "gatsby-plugin-sharp": "^2.6.10",
    "gatsby-plugin-styled-components": "^3.3.3",
    "gatsby-plugin-typography": "^2.5.3",
    "gatsby-remark-images": "^3.3.9",
    "gatsby-remark-katex": "^3.3.4",
    "gatsby-source-filesystem": "^2.3.10",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.5.4",
    "katex": "^0.11.1",
    "netlify-cms-app": "^2.12.14",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-typography": "^0.16.19",
    "styled-components": "^5.1.1",
    "twin.macro": "^1.3.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.8.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.4",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-react": "7.20.0",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "2.0.5",
    "tailwindcss": "1.4.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  }
}
